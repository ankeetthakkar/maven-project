pipeline
{
  agent any
  stages
  {
     stage('clone my code')
	 {
	   steps
	   {
	     git branch: 'master', url: 'https://github.com/ankeetthakkar/maven-project.git'
	   }
	   
	 }
	 stage('install maven')
	 { {lavel: 'maven'}
	   steps
	   {
	     with maven(jdk: 'jdk' maven:'localmaven')
	   }
	 }
  }
}
